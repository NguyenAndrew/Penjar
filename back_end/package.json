{
  "name": "back_end",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "tsc --outDir build && copyfiles --up 1 --exclude \"src/**/*.ts\" \"src/**/*\" build",
    "clean:build": "rimraf build",
    "start": "npm run start:cloud:linux",
    "test": "jest",
    "start:cloud:windows": "set NODE_ENV=cloud&& node build/server.js",
    "start:cloud:linux": "NODE_ENV=cloud node build/server.js",
    "start:cloud:mac": "NODE_ENV=cloud node build/server.js",
    "start:local:windows": "set NODE_ENV=local&& nodemon --ignore build/ --ext \"*\" --exec \"npm run build && node build/server.js\"",
    "start:local:linux": "NODE_ENV=local nodemon --ignore build/ --ext \"*\" --exec \"npm run build && node build/server.js\"",
    "start:local:mac": "NODE_ENV=local nodemon --ignore build/ --ext \"*\" --exec \"npm run build && node build/server.js\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.3",
    "copyfiles": "^2.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.3"
  }
}
