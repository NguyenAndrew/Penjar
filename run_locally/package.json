{
  "name": "run_locally",
  "version": "1.0.0",
  "description": "Runs the project locally",
  "main": "index.js",
  "scripts": {
    "postinstall": "concurrently \"cd ../back_end && npm install\" \"cd ../front_end && npm install\"",
    "start": "concurrently \"cd ../back_end && npm run start:local:windows\" \"cd ../front_end && npm run start:local\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
